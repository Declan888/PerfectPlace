<div id="sideBar">
    <a href="#" id="sideBarTab" data-toggle="modal" data-target="#myModal2"><img src="/Image/infographic/slide-button.gif" alt="sideBar" title="sideBar" /></a>
    <!-- Modal -->
    <div class="modal left fade" id="myModal2" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog" role="document">
            <div class="modal-content" style="width:780px;">

                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="myModalLabel">Compare</h4>
                </div>

                <div class="modal-body">

                    <form action="Compare">
                        <table id="example" class="table table-bordered" cellspacing="0" width="100%">
                            <thead>
                            @if (@ViewBag.SearchResultPage == "SearchByLifeStyle")
                            {
                                @:@if (@ViewBag.LifeStyle == "SeaChange")
                                  {
                                      <tr>
                                          <th>&nbsp;&nbsp;&nbsp;</th>
                                          <th>Suburb</th>
                                          <th>Frequency of Public Transportation (min)</th>
                                          <th>Number of Golf Court</th>
                                          <th>Distance to the Fishing Spots (Km)</th>
                                      </tr>
                                  }
                                  else if (@ViewBag.LifeStyle == "TreeChange")
                                  {
                                      <tr>
                                          <th>&nbsp;&nbsp;&nbsp;</th>
                                          <th>suburb</th>
                                          <th>Distance to the Nearest Hospital (Km)</th>
                                          <th>Frequency of Public Transportation (min)</th>
                                          <th>Number of Nursery Shops</th>
                                      </tr>
                                  }
                                  else if (@ViewBag.LifeStyle == "CityChange")
                                  {
                                      <tr>
                                          <th>&nbsp;&nbsp;&nbsp;</th>
                                          <th>Suburb</th>
                                          <th>Apartment Price (2 bedrooms)</th>
                                          <th>Number of Cafe</th>
                                          <th>Number of Dining</th>
                                          <th>Number of Bar & Club</th>
                                      </tr>
                                  }
                            }
                            else
                            {
                                <tr>
                                    <th>&nbsp;&nbsp;&nbsp;</th>
                                    <th>Suburb</th>
                                    <th>Distance to CBD (Km)</th>
                                    <th>Commercial Rate</th>
                                    <th>Health Service</th>
                                    <th>Accident Rate</th>
                                    <th>Age Care Center</th>
                                    <th>Time To Hospital (min)</th>
                                    <th>criminal Rate</th>
                                </tr>
                            }

                            </thead>
                            <tbody>
                            @foreach (var item in Model.SuburbListData)
                            {
                                @:@if (@ViewBag.SearchResultPage == "SearchByLifeStyle")
                                  {
                                      @:@if (@ViewBag.LifeStyle == "SeaChange")
                                        {
                                            <tr>
                                                <td><input class="compareCheckBox" type="checkbox" name="suburbb" value="@item.suburb_id" />&nbsp;</td>
                                                <td>@Html.ActionLink(@item.suburb, "Details", "Suburb", new { id = item.suburb_id, lay = 1 }, null) </td>
                                                <td>@item.Public_Transport_Freq</td>
                                                <td>@item.golf_count</td>
                                                <td>@Math.Round(item.distance_to_fishspot.Value, 1)</td>
                                            </tr>
                                        }
                                        else if (@ViewBag.LifeStyle == "TreeChange")
                                        {
                                            <tr>
                                                <td><input class="compareCheckBox" type="checkbox" name="suburbb" value="@item.suburb_id" />&nbsp;</td>
                                                <td>@Html.ActionLink(@item.suburb, "Details", "Suburb", new { id = item.suburb_id, lay = 1 }, null) </td>
                                                <td>@Math.Round(item.distance_nearest_public_hospital.Value, 1)</td>
                                                <td>@item.Public_Transport_Freq</td>
                                                <td>@item.nursery_shops</td>
                                            </tr>
                                        }
                                        else if (@ViewBag.LifeStyle == "CityChange")
                                        {
                                            <tr>
                                                <td><input class="compareCheckBox" type="checkbox" name="suburbb" value="@item.suburb_id" />&nbsp;</td>
                                                <td>@Html.ActionLink(@item.suburb, "Details", "Suburb", new { id = item.suburb_id, lay = 1 }, null) </td>
                                                <td>$@item.buy_unit_2br_string</td>
                                                <td>@item.cafes_count</td>
                                                <td>@item.dining_count</td>
                                                <td>@item.bar_count</td>
                                            </tr>
                                        }
                                  }
                                  else
                                  {
