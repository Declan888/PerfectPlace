    https://maps.googleapis.com/maps/api/geocode/json?address=" + val + "&region=es&key=AIzaSyB1F_18NdPhEf8BftCYLByeaXZzmnFDb5A
        function GetLatLng(val) {

            var latlng = [];

            jQuery.ajax({
                url: "https://maps.googleapis.com/maps/api/geocode/json?address=" + val + ",VIC" + "&%E2%80%8Cregion=es&key=AIzaSyB1F_18NdPhEf8BftCYLByeaXZzmnFDb5A&components=country:AU",
                dataType: 'json',
                async: false,
                data: null,
                success: function (result) {
                    var lat = '';
                    var lng = '';
                    jQuery.each(result.results["0"].geometry.location, function (key, val) {

                        if (key == 'lat')
                        { lat = val; }
                        else if (key == 'lng')
                        { lng = val; }

                    });
                    latlng.push({ "lat": "" + lat + "", "lng": "" + lng + "" });


                    jQuery.each(result.results["0"].geometry.bounds.northeast, function (key, val) {

                        if (key == 'lat')
                        { lat = val; }
                        else if (key == 'lng')
                        { lng = val; }

                    });
                    bound_cmp.push({ "lat": "" + lat + "", "lng": "" + lng + "" });
                    jQuery.each(result.results["0"].geometry.bounds.southwest, function (key, val) {

                        if (key == 'lat')
                        { lat = val; }
                        else if (key == 'lng')
                        { lng = val; }

                    });
                    bound_cmp.push({ "lat": "" + lat + "", "lng": "" + lng + "" });

                    completeName.push(result.results["0"].formatted_address);

                }
            });

            return latlng;
        }


    function GetValueUsingKey(arr, keyy) {
        var res = 0;
        jQuery.each(arr[0], function (key, val) {


            if (keyy == key)
                res = val;

        });

        return res;
    }
    function GetValueUsingKey2(arr, keyy) {
        var res = 0;
        jQuery.each(arr, function (key, val) {


            if (keyy == key)
                res = val;

        });

        return res;
    }















    var lat1 = GetValueUsingKey(latlng_cmp[0], 'lat');
    var long1 = GetValueUsingKey(latlng_cmp[0], 'lng');
    var lat2 = GetValueUsingKey(latlng_cmp[1], 'lat');
    var long2 = GetValueUsingKey(latlng_cmp[1], 'lng');


    var locations = [
        ["" + completeName[0] + "", lat1, long1],
        ["" + completeName[1] + "", lat2, long2]
    ];

    //var locations = [
